package org.iitb.exam.main.wrappers;

import java.util.ArrayList;
import java.util.Date;

public class ExamDetail {
	
	
	/**
	 * This class is used to set and get the details of 
	 * particular exam 
	 * 
	 * @author dipti
	 */
	
	/*
	 * @param examId - for storing the Exam ID
	 * @param examName- For storing the Exam name
	 * @param examDate- for storing exam date
	 * @param examDuration - For storing exam duration
	 * @param QuestionList - For storing Question detail belonging to this exam
	 * 
	 */
	
	private int examId;
	private String examName;
	private Date examDate;
	private int examDuration;
	private String designationName;
	private ArrayList<Question> QuestionList=new ArrayList <Question>();
	/**
	 * @return the examId
	 */
	public int getExamId() {
		return examId;
	}
	/**
	 * @param examId the examId to set
	 */
	public void setExamId(int examId) {
		this.examId = examId;
	}
	/**
	 * @return the examName
	 */
	public String getExamName() {
		return examName;
	}
	/**
	 * @param examName the examName to set
	 */
	public void setExamName(String examName) {
		this.examName = examName;
	}
	/**
	 * @return the examDate
	 */
	public Date getExamDate() {
		return examDate;
	}
	/**
	 * @param examDate the examDate to set
	 */
	public void setExamDate(Date examDate) {
		this.examDate = examDate;
	}
	/**
	 * @return the examDuration
	 */
	public int getExamDuration() {
		return examDuration;
	}
	/**
	 * @param examDuration the examDuration to set
	 */
	public void setExamDuration(int examDuration) {
		this.examDuration = examDuration;
	}
	
	
	public String getDesignationName() {
		return designationName;
	}
	public void setDesignationName(String designationName) {
		this.designationName = designationName;
	}
	/**
	 * @return the questionList
	 */
	public ArrayList<Question> getQuestionList() {
		return QuestionList;
	}
	/**
	 * @param questionList the questionList to set
	 */
	public void setQuestionList(ArrayList<Question> questionList) {
		QuestionList = questionList;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "ExamDetail [examId=" + examId + ", examName=" + examName
				+ ", examDate=" + examDate + ", examDuration=" + examDuration
				+ ", designationName=" + designationName + ", QuestionList="
				+ QuestionList + "]";
	}
	
	

}
